$ echo "CONTENT"
  echo "Hello World"

$ NAME=VALUE 
$ echo $NAME
VALUE

[Examples]
$ name=DY
$ echo $name
DY


echo -n 'CONTENT' > FILE_NAME                                                               # save the content in a file
echo -n 'CONTENT' > ./FILE_NAME

[Examples]
$ echo -n 'hello world' > FILE_NAME
