$ chmod +x *
$ chmod +x FILE_NAME
-rwxrwxr-x 1 ubuntu ubuntu 54 Aug  7 12:08 FILE_NAME
$ chmod +x FILE_NAME.EXTENSION
  chmod +x FILE_NAME.sh
$ chmod +x FOLDER_NAME/FILE_NAME
$ chmod +r FOLDER_NAME*
$ chmod -wx FILE_NAME*
$ chmod +rwx FILE_NAME
$ chmod -rwx FOLDER_NAME
$ chmod u+x FILE_NAME
  chmod u+x FILE_NAME.sh
$ chmod u+x ./FOLDER_NAME/FILE_NAME
$ chmod u+x ./FOLDER_NAME/FILE_NAME.EXTENSION
  chmod u+x ./FOLDER_NAME/FILE_NAME.sh
$ chmod a+x FILE_NAME
  chmod a+x FILE_NAME.sh

$ chmod 777 FILE_NAME
$ chmod 777 FILE_NAME.EXTENSION
  chmod 777 FILE_NAME.sh
